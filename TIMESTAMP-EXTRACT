SHOW TIMEZONE;

SELECT NOW();

SELECT TIMEOFDAY();

SELECT CURRENT_TIME;

SELECT CURRENT_DATE;

SELECT * FROM payment;

SELECT EXTRACT(YEAR FROM payment_date) 
AS year
FROM payment;

SELECT EXTRACT(QUARTER FROM payment_date) 
AS month
FROM payment;

SELECT AGE(payment_date)
FROM payment;

SELECT TO_CHAR(payment_date,'dd-MM-YYYY')
FROM payment;